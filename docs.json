{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "HNZRP STG",
  "colors": {
    "primary": "#16A34A",
    "light": "#07C983",
    "dark": "#15803D"
  },
  "favicon": "/stg.ico",
  "navigation": {
    "tabs": [
      {
        "tab": "Main",
        "groups": [
          {
            "group": "STG - Information",
            "pages": ["index", "/dashboard/sops"]
          }
        ]
      },
      {
        "tab": "Standard Operating Procedures",
        "groups": [
          {
            "group": "STG - SOPs",
            "pages": [
              "/sops/deployment-policy",
              "/sops/vehicles-and-equipment",
              "/sops/ranks-and-callsigns",
              "/sops/behaviour"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Documentation",
          "href": "https://mintlify.com/docs",
          "icon": "book-open-cover"
        },
        {
          "anchor": "Blog",
          "href": "https://mintlify.com/blog",
          "icon": "newspaper"
        }
      ]
    }
  },
  "logo": {
